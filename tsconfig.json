{
  "compilerOptions": {
    "jsx": "react-jsx", // Use "react-jsx" if using React 17+ and JSX runtime, otherwise "react"
    "module": "ESNext",
    "target": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node"
  }
}
